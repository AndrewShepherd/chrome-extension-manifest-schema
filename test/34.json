{
  "name": "Test IME",
  "version": "1.0",
  "manifest_version": 2,
  "description": "A simple IME that converts all keystrokes to upper case.",
  "background":  {
    "scripts": ["main.js"]
  },
  "permissions": [
    "input"
  ],
  "input_components": [
    {
      "name": "Test IME",
      "type": "ime",
      "id": "test",
      "description": "Test IME",
      "language": "en-US",
      "layouts": ["us::eng"]
    }
  ]
}
